    "@types/luxon": "^3.4.2",
    "@types/mustache": "^4.2.5",
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.2.5",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@mapbox/polyline": "1.2.1",
    "@types/handlebars": "^4.1.0",
    "@types/mapbox__polyline": "^1.0.5",
